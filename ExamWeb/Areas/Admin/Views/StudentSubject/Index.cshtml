<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">StudentSubject List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="StudentSubject" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> &nbsp; Create New StudentSubject
            </a>
        </div>
    </div>
    <br /><br />

    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Display Order</th>
                <th>ExamCode</th>
                <th>Student</th>
                <th>Subject</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/StudentSubject.js"></script>
}